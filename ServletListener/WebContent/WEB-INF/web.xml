	<param-value>root</param-value>
</context-param>
<context-param>
	<param-name>DBPASSWORD</param-name>
	<param-value>43grhn25</param-value>
</context-param>
<context-param>
	<param-name>DBURL</param-name>
	<param-value>jdbc:mysql://localhost/deneme</param-value>
